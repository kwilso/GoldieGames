
@{
    ViewData["Title"] = "ItemsList";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Board Games in Our Inventory</h2>

@model IEnumerable<BoardGame>


    <table class="table table-sm table-striped table-bordered">
        <thead>
            <tr>
                <th>Name</th>
                <th>Seller</th>
                <th>Price</th>
                <th>Genre</th>
            </tr>
        </thead>
        <tbody>
            @foreach (BoardGame obj in Model)
        {
            <tr>
                <td>@obj.Title</td>
                <td>@obj.Seller</td>
                <td>@obj.Price.ToString()</td>
                <td>@obj.Genre</td>
            </tr>
        }

        </tbody>
    </table>

